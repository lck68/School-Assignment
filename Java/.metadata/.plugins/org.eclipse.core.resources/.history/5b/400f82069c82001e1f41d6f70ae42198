package 大数据G226_03;

class Person2{
	int age;
	String name;
	public Person2(int a,String b){
		age = a;
		name = b;
	}
public void speak(){
	System.out.println("我的学号是"+age+"号");
	System.out.println("我的名字"+name);
}
}
public class Item_11_14_3 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person2 p1=new Person2(20,"刘大林");
		p1.speak();

	}

}
